<?xml version="1.0"?>

<!-- hydrualic pressure gauge -->

<PropertyList>

 <path>Pressure_gauge.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>Needle</object-name>
  <property>/controls/hydraulic/system/engine-pump</property>
  <interpolation>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>100</ind><dep>270</dep></entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Knob</object-name>
  <property>/controls/hydraulic/system/engine-pump</property>
  <factor>36</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0.007</x>
   <y>-0.01906</y>
   <z>-0.01874</z>
  </axis>
 </animation>

<animation n="100">
 <type>scale</type>
 <property>constants/zero</property>
 <x-offset>1.2</x-offset>
 <y-offset>1.2</y-offset>
 <z-offset>1.2</z-offset>
</animation>

</PropertyList>
