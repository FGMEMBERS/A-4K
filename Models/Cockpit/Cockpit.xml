 <object-name>spd_brk_open_light</object-name>
 <object-name>spoiler_ext_light</object-name>
 <object-name>dc_1_light</object-name>
 <object-name>dc_2_light</object-name>
 <type>material</type>
 <emission>
  <red>5</red>
  <green>5</green>
  <blue>5</blue>
  </emission>
</animation>

<animation>
 <type>select</type>
 <object-name>canopy_light</object-name>
 <condition>
  <or>
   <equals>
    <property>/instrumentation/doors/crew/position-norm</property>
    <value>1</value>
   </equals>
  </or>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>brake_light</object-name>
 <condition>
  <or>
   <equals>
    <property>/controls/gear/brake-parking</property>
    <value>1</value>
   </equals>
  </or>
 </condition>
</animation>

<animation>
<type>select</type>
<object-name>master_caution_light</object-name>
<condition>
 <less-than>
  <property>consumables/fuel/total-fuel-gal_us</property>
  <value>30</value>
 </less-than>
 <or>
  <equals>
   <property>/bombable/fire-particles/fire-burning</property>
   <value>true</value>
  </equals>
  <equals>
   <property>/engines/engine/out-of-fuel</property>
   <value>true</value>
  </equals>
 </or>
</condition>
</animation>

<animation>
<type>select</type>
<object-name>fire_light</object-name>
<condition>
  <or>
   <equals>
    <property>/bombable/fire-particles/fire-burning</property>
    <value>true</value>
   </equals>
  </or>
</condition>
</animation>
